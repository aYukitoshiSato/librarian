<div class="main">

  <h1>リストに追加する</h1>

  <%= form_tag ("/lists/#{params[:id]}/add") do %>
    <p>追加するリスト</p>
    <%= collection_select :contents_list, :list_id, List.where(user_id: session[:user_id]), :id, :name %>
    <input type="submit" value="追加" class="square_btn post_btn">
  <% end %>
  <p>
    <%= link_to("新規リストの作成",
      "/lists/new",
      class: "square_btn normal_btn") %>
  </p>
</div>
